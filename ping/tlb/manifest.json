{
  "name": "TLB",
  "description": "Translation Lookaside Buffer aligned with Akita's tlb builder.",
  "ports": [
    {
      "name": "Top"
    },
    {
      "name": "Bottom"
    }
  ],
  "parameters": [
    {
      "name": "frequency",
      "type": "float",
      "unit": "Hz",
      "default": 1000000000
    },
    {
      "name": "numWays",
      "type": "int",
      "default": 8
    },
    {
      "name": "numSets",
      "type": "int",
      "default": 8
    },
    {
      "name": "log2PageSize",
      "type": "int",
      "default": 12
    },
    {
      "name": "numReqPerCycle",
      "type": "int",
      "default": 2
    },
    {
      "name": "translationProviderMapperType",
      "type": "string",
      "default": "single"
    },
    {
      "name": "translationProviders",
      "type": "port",
      "default": "null"
    }
  ]
}